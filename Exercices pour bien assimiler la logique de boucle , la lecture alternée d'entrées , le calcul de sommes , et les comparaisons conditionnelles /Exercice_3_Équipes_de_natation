nombre_de_nageurs_par_équipe = int(input())
équipe_1 = 0
équipe_2 = 0
for i in range(nombre_de_nageurs_par_équipe * 2):
    temps_individuels = int(input())
    if i % 2 == 0:
        équipe_1 = équipe_1 + temps_individuels
    else:
        équipe_2 = équipe_2 + temps_individuels
if équipe_1 < équipe_2:
    print("L'équipe 1 a un avantage")
else:
    print("L'équipe 2 a un avantage")
print("Temps total équipe 1 : ", équipe_1)
print("Temps total équipe 2 : ", équipe_2)